generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model AnalysisResult {
  id              String   @id @default(uuid())
  filename        String
  transcribedText String
  score           Int
  feedback        String
  createdAt       DateTime @default(now())
}
